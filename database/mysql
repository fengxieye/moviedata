CREATE TABLE `movie` (
	`id` BIGINT(19) NOT NULL AUTO_INCREMENT,
	`name` VARCHAR(50) NOT NULL DEFAULT '' COMMENT '名字' COLLATE 'utf8mb4_general_ci',
	`cover` INT(10) NULL DEFAULT NULL COMMENT '封面',
	`start` TINYINT(3) NULL DEFAULT NULL COMMENT '星星',
	`count` INT(10) NULL DEFAULT NULL COMMENT '评论人数',
	`value` FLOAT NULL DEFAULT NULL COMMENT '评分',
	`kind` TINYINT(3) NULL DEFAULT '0' COMMENT '0-电影，1-剧',
	`tag` VARCHAR(50) NULL DEFAULT '' COMMENT '类型' COLLATE 'utf8mb4_general_ci',
	`doubanid` BIGINT(19) NULL DEFAULT NULL COMMENT '豆瓣id',
	`t_version` TINYINT(3) NULL DEFAULT '0' COMMENT '热度版本',
	`t` INT(10) NULL DEFAULT NULL COMMENT '热度',
	PRIMARY KEY (`id`) USING BTREE,
	UNIQUE INDEX `name` (`name`) USING BTREE,
	UNIQUE INDEX `doubanid` (`doubanid`) USING BTREE
)
COLLATE='utf8mb4_0900_ai_ci'
ENGINE=InnoDB
AUTO_INCREMENT=4
;

